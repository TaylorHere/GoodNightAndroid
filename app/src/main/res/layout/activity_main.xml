<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bk"
    tools:context="com.example.taylor.goodnightfm.Views.MainActivity">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/Main_layout_recyclerView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="31dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="33dp"
        android:background="@color/bk"
        android:fadeScrollbars="false"
        android:fadingEdge="none"
        android:scrollIndicators="none"
        android:scrollbarAlwaysDrawHorizontalTrack="false"
        android:scrollbarAlwaysDrawVerticalTrack="false"
        app:layout_constraintBottom_toTopOf="@+id/indicator"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/moon"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/moon"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginEnd="170dp"
        android:layout_marginLeft="170dp"
        android:layout_marginRight="170dp"
        android:layout_marginStart="170dp"
        android:layout_marginTop="45dp"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/moon" />

    <ImageView
        android:id="@+id/indicator"
        android:layout_width="40dp"
        android:layout_height="8dp"
        android:layout_marginBottom="83dp"
        android:layout_marginEnd="187dp"
        android:layout_marginLeft="187dp"
        android:layout_marginRight="187dp"
        android:layout_marginStart="187dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.433"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@mipmap/line" />

</android.support.constraint.ConstraintLayout>
